{
  "welcomeFile": "/index.html",
  "authenticationMethod": "none",
  "routes": [
    {
      "source": "^/odata/(.*)$",
      "target": "/odata/$1",
      "destination": "srv-api",
      "csrfProtection": false
    },
    {
      "source": "^(.*)$",
      "target": "/$1",
      "localDir": "resources"
    }
  ]
}
